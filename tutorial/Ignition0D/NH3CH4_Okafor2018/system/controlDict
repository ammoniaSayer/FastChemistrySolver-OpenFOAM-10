FoamFile
{
    format      ascii;
    class       dictionary;
    location    "system";
    object      controlDict;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

application     chemFoam;

startFrom       startTime;

startTime       0;

stopAt          endTime;

endTime         0.1;

deltaT          1e-08;

maxDeltaT       1e-4;

adjustTimeStep  on;

writeControl    adjustableRunTime;

writeInterval   5e-01;

purgeWrite      0;

writeFormat     ascii;

writeCompression off;

timeFormat      general;

timePrecision   16;

runTimeModifiable yes;

DebugSwitches
{
    SolverPerformance   0;
}
libs
(
"libFastChemistryModel.so"
"libPLOGReactions.so"
);

