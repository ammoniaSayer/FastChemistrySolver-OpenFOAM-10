limitp
{
    type       limitPressure;

    min  100000;
    max  102000;
}


    limitT
    {
        type            limitTemperature;

        selectionMode   all;

        // field           T;   // Optional energy/temperature field name
                                // Set to T for compressibleInterFoam

 //       phase           gas; // Optional phase name

        min             290;
        max             2400;
    }
limitU
{
        type limitMag;
        selectionMode   all;
        field   U;
        max     80;
