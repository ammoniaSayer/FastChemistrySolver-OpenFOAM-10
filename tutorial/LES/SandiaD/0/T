FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 300;

boundaryField
{
    inletAir
    {
        type            fixedValue;
        value           uniform 291;
    }
    inletCH4
    {
        type            fixedValue;
        value           uniform 294;
    }
    inletPilot
    {
        type            fixedValue;
        value           uniform 1880;
    }
    outlet
    {
        type            zeroGradient;
    }
    side
    {
        type            zeroGradient;
    }
    wallTube
    {
        type            zeroGradient;
    }
}


