FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 101325;

boundaryField
{
    inletAir
    {
        type            zeroGradient;
    }
    inletCH4
    {
        type            zeroGradient;
    }
    inletPilot
    {
        type            zeroGradient;
    }
    outlet
    {
        type            waveTransmissive;
        gamma           1.4;
        fieldInf        101325;
        lInf            0.3;
        value           uniform 101325;
    }
    side
    {
        type            waveTransmissive;
        gamma           1.4;
        fieldInf        101325;
        lInf            0.3;
        value           uniform 101325;
    }
    wallTube
    {
        type            zeroGradient;
    }
}


