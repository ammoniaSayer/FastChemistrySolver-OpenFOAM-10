FoamFile
{
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0.9 0 0);

boundaryField
{
    inletAir
    {
        type            fixedValue;
        value           uniform (0.9 0 0);
    }
    inletCH4
    {
        type            turbulentGenerator;
        direction       1;
        value           uniform (49.6 0 0);
    }
    inletPilot
    {
        type            fixedValue;
        value           uniform (11.4 0 0);
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (0.9 0 0);
    }
    side
    {
        type            inletOutlet;
        inletValue      uniform (0 0 0);
        value           uniform (0.9 0 0);
    }
    wallTube
    {
        type            noSlip;
    }
}


