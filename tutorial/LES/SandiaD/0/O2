FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      O2;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.23;

boundaryField
{
    inletAir
    {
        type            fixedValue;
        value           uniform 0.23;
    }
    inletCH4
    {
        type            fixedValue;
        value           uniform 0.1966;
    }
    inletPilot
    {
        type            fixedValue;
        value           uniform 0.054;
    }
    outlet
    {
        type            zeroGradient;
    }
    side
    {
        type            zeroGradient;
    }
    wallTube
    {
        type            zeroGradient;
    }
}


