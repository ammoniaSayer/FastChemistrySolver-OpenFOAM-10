FoamFile
{
    format          ascii;
    class           dictionary;
    location        "constant";
    object          chemistryProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

chemistryType
{
    //solver          ode;
    solver          OptSeulex;
    method          FastChemistryModel;
}

chemistry       on;

initialChemicalTimeStep 1e-07;

jacobian        exact;

balance         on;

Treact 0;

odeCoeffs
{
    solver          seulex;
    absTol          1e-8;
    relTol          1e-4;
}

OptSeulexCoeffs
{
    absTol          1e-8;
    relTol          1e-4;
}

#include        "reactionsDRM19"


