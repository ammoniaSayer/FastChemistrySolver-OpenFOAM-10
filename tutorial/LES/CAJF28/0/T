FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 294;

boundaryField
{
    inlet-1
    {
        type            fixedValue;
        value           uniform 294;
    }
    inlet-2
    {
        type            fixedValue;
        value           uniform 294;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 294;
        value           uniform 294;
    }
    wall-1
    {
        type            zeroGradient;
    }
}


