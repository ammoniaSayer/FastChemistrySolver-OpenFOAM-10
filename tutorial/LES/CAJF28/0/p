FoamFile
{
    version     2.0;
    format      ascii;
    arch        "LSB;label=32;scalar=64";
    class       volScalarField;
    location    "0";
    object      p;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [1 -1 -2 0 0 0 0];

internalField   uniform 500000;

boundaryField
{
    inlet-1
    {
        type            zeroGradient;
    }
    inlet-2
    {
        type            zeroGradient;
    }
    outlet
    {
        type            waveTransmissive;
        gamma           1.3;
        fieldInf        500000;
        lInf            0.3;
        value           uniform 500000;
    }
    wall-1
    {
        type            zeroGradient;
    }
}


