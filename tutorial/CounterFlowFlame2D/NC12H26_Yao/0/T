FoamFile
{
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 2000;

boundaryField
{
    #includeEtc "caseDicts/setConstraintTypes"

    fuel
    {
        type            fixedValue;
        value           uniform 500;
    }
    air
    {
        type            fixedValue;
        value           uniform 500;
    }
    outlet
    {
        type            inletOutlet;
        inletValue      uniform 500;
        value           uniform 500;
    }
    frontAndBack
    {
        type            empty;
    }
}


