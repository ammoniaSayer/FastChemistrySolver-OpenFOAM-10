chemistryType
{
    //solver          ode;
    solver          OptRodas34;
    method          FastChemistryModel;
}

Treact 0;

jacobian        exact;

chemistry       on;

initialChemicalTimeStep 1e-07;

odeCoeffs
{
    solver          rodas34;
    absTol          1e-8;
    relTol          1e-4;
}
OptRodas34Coeffs
{
    absTol          1e-8;
    relTol          1e-4;
}

